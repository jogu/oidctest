{
  "group": "ID Token",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    {
      "Registration": {
        "set_request_args": {
          "id_token_encrypted_response_alg": "RSA1_5",
          "id_token_encrypted_response_enc": "A128CBC-HS256",
          "id_token_signed_response_alg": "RS256"
        },
        "check_support": {
          "ERROR": {
            "id_token_encryption_enc_values_supported": "A128CBC-HS256",
            "id_token_encryption_alg_values_supported": "RSA1_5",
            "id_token_signing_alg_values_supported": "RS256"
          }
        }
      }
    },
    {
      "AsyncAuthn": {
        "set_response_where": null
      }
    },
    {
      "AccessToken": {
        "conditional_execution": {
          "return_type": [
            "CIT",
            "CI",
            "C",
            "CT"
          ]
        }
      }
    }
  ],
  "usage": {
    "sig": true,
    "enc": true,
    "register": true,
    "extra": true
  },
  "desc": "Signed and encrypted ID Token [Extra]",
  "assert": {
    "signed-encrypted-idtoken": {
      "enc_alg": "RSA1_5",
      "enc_enc": "A128CBC-HS256",
      "sign_alg": "RS256"
    },
    "verify-response": {
      "response_cls": [
        "AuthorizationResponse",
        "AccessTokenResponse"
      ]
    }
  },
  "reference": [
    "https://openid.net/specs/openid-connect-core-1_0.html#IDToken",
    "https://openid.net/specs/openid-connect-core-1_0.html#Signing",
    "https://openid.net/specs/openid-connect-registration-1_0.html#ClientMetadata",
    "https://openid.net/specs/openid-connect-core-1_0.html#Encryption"
  ]
}
