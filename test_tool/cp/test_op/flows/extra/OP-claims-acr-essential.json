{
  "group": "claims Request Parameter",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    "Registration",
    {
      "AsyncAuthn": {
        "set_response_where": null,
        "set_request_args": {
          "claims": {
            "id_token": {
              "acr": {
                "essential": true
              }
            }
          }
        }
      }
    },
    {
      "AccessToken": {
        "conditional_execution": {
          "return_type": [
            "CIT",
            "CI",
            "C",
            "CT"
          ]
        }
      }
    }
  ],
  "usage": {
    "extra": true
  },
  "desc": "Requesting ID Token with essential acr claim [Extra]",
  "assert": {
    "verify-claims": {
      "id_token": null
    },
    "verify-response": {
      "response_cls": [
        "AccessTokenResponse",
        "AuthorizationResponse"
      ]
    }
  },
  "reference": [
    "https://openid.net/specs/openid-connect-core-1_0.html#ClaimsParameter",
    "https://openid.net/specs/openid-connect-core-1_0.html#acrSemantics"
  ]
}
