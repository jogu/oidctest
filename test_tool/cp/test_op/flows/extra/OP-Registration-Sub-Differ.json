{
  "group": "Dynamic Client Registration",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    {
      "Registration": {
        "set_request_args": {
          "subject_type": "public"
        },
        "check_support": {
          "ERROR": {
            "subject_types_supported": "public"
          }
        }
      }
    },
    {
      "AsyncAuthn": {
        "set_response_where": null
      }
    },
    {
      "AccessToken": {
        "conditional_execution": {
          "return_type": [
            "CIT",
            "CI",
            "C",
            "CT"
          ]
        }
      }
    },
    {
      "Registration": {
        "set_request_args": {
          "subject_type": "pairwise"
        },
        "check_support": {
          "ERROR": {
            "subject_types_supported": "pairwise"
          }
        }
      }
    },
    {
      "AsyncAuthn": {
        "set_response_where": null
      }
    },
    {
      "AccessToken": {
        "conditional_execution": {
          "return_type": [
            "CIT",
            "CI",
            "C",
            "CT"
          ]
        }
      }
    }
  ],
  "usage": {
    "register": true,
    "extra": true
  },
  "desc": "Public and pairwise sub values differ",
  "assert": {
    "verify-response": {
      "response_cls": [
        "AuthorizationResponse",
        "AccessTokenResponse"
      ]
    },
    "different_sub": null
  }
}
